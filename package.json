{
  "name": "ember-cli-html-minifier",
  "version": "1.0.0",
  "description": "Uses html-minifier to minify your ember-cli app's html as well as any inline js/css",
  "main": "index.js",
  "author": "Garth Poitras <garth22@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:gpoitch/ember-cli-html-minifier.git"
  },
  "keywords": [
    "ember-addon",
    "minify",
    "compress",
    "html",
    "html-minifier"
  ],
  "ember-addon": {
    "after": [
      "ember-cli-fastboot"
    ]
  },
  "dependencies": {
    "broccoli-filter": "^1.3.0",
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "^3.0.1",
    "html-minifier-terser": "^5.0.2",
    "util-extend": "^1.0.3"
  }
}
